    "baucis": "~0.4.6",
    "express-hbs": "~0.2.0",
    "is-my-json-valid": "^2.13.1",
    "jquery": "^2.2.4",
    "lodash": "^4.13.1",
    "bootstrap-sass": "^3.3.6",
    "underscore": "~1.8.3"
  },
  
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "bower": "~1.8.0",
    "grunt": "~1.0.1",
    "grunt-contrib-copy": "~0.4.0",
    "grunt-contrib-concat": "~0.2.0",
    "grunt-contrib-coffee": "~0.6.6",
    "grunt-contrib-handlebars": "~0.5.8",
